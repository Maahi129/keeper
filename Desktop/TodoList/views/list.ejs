<%- include("header") -%>

    <div class="box" id="heading">
      <h1> <%= ListTitle %></h1>
    </div>
<div class="box">
    <%  for (var i=0; i<Items.length; i++){ %>
  <div class="item">
    <input type="checkbox">
    <p><%= Items[i] %></p>
  </div>
  <%  } %>

  <form class="item" action="/" method="post">
  <input type="text" name="new" placeholder="New Item" autocomplete="off">
  <button type="submit" name="list" value = <%= ListTitle %> >+</button>

</div>

</form>

<%- include("footer") -%>
